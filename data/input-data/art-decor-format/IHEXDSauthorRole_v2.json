{ "valueSet" : [{ "id" : "1.2.276.0.76.11.30", "name" : "IHEXDSauthorRole", "displayName" : "IHE XDS authorRole", "statusCode" : "final", "versionLabel" : "v2", "canonicalUri" : "http://ihe-d.de/ValueSets/IHEXDSauthorRole", "effectiveDate" : "2018-07-13T13:12:46", "desc" : [{ "language" : "en-US", "#text" : "&lt;b&gt;authorRole (XDSDocumentEntry or XDSSubmissionSet)&lt;/b&gt;\n&lt;br/&gt;Represents the role of the author with respect to the patient at the time that an XDS Document or XDS Submission Set was created. " }], "sourceCodeSystem" : [{ "id" : "1.3.6.1.4.1.19376.3.276.1.5.13", "identifierName" : "Prozessrollen&#160;für&#160;Autoren", "canonicalUri" : "urn:oid:1.3.6.1.4.1.19376.3.276.1.5.13", "canonicalUriDSTU2" : "http://ihe-d.de/CodeSystems/ProzessrollenFuerAutoren", "canonicalUriSTU3" : "http://ihe-d.de/CodeSystems/ProzessrollenFuerAutoren", "canonicalUriR4" : "http://ihe-d.de/CodeSystems/ProzessrollenFuerAutoren" }, { "id" : "1.3.6.1.4.1.19376.3.276.1.5.14", "identifierName" : "Patientenbeziehungsrollen&#160;für&#160;Autoren", "canonicalUri" : "urn:oid:1.3.6.1.4.1.19376.3.276.1.5.14", "canonicalUriDSTU2" : "http://ihe-d.de/CodeSystems/PatientenbeziehungsrollenFuerAutoren", "canonicalUriSTU3" : "http://ihe-d.de/CodeSystems/PatientenbeziehungsrollenFuerAutoren", "canonicalUriR4" : "http://ihe-d.de/CodeSystems/PatientenbeziehungsrollenFuerAutoren" }], "publishingAuthority" : [{ "name" : "IHE Deutschland e.V.", "addrLine" : ["Charlottenstr. 42", "10117 Berlin", "Fon: +49 (0)30 / 22 00 24-70", "Fax: +49 (0)30 / 22 00 24-799", "Internet: http://www.ihe-d.de"] }], "conceptList" : [{ "concept" : [{ "code" : "1", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.13", "displayName" : "Einweiser", "level" : "0", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "Die Person, die die Einweisung veranlasst hat. (Sie wird auch als Zuweiser bezeichnet.)&lt;br clear=\"none\"/&gt;" }] }, { "code" : "2", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.13", "displayName" : "Entlassender", "level" : "0", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "Die Person, die für die Entlassung verantwortlich ist.&lt;br clear=\"none\"/&gt;" }] }, { "code" : "3", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.13", "displayName" : "Überweiser", "level" : "0", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "Die Person, die die Überweisung veranlasst hat.&lt;br clear=\"none\"/&gt;" }] }, { "code" : "4", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.13", "displayName" : "Durchführender", "level" : "0", "type" : "S", "desc" : [{ "language" : "de-DE", "#text" : "Die Person oder das Gerät, welche(s) die dokumentierte Aktion durchgeführt hat.&lt;br clear=\"none\"/&gt;" }] }, { "code" : "5", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.13", "displayName" : "durchführendes Gerät", "level" : "1", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "Das Gerät, welches die dokumentierte Aktion durchgeführt hat.&lt;br clear=\"none\"/&gt;" }] }, { "code" : "6", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.13", "displayName" : "Betreuer", "level" : "1", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "Die Person, die den Patienten betreut, d.h. den Überblick über die Behandlung behält und als primärer Ansprechpartner während der Behandlung dient.&amp;#160;&lt;br clear=\"none\"/&gt;" }] }, { "code" : "7", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.13", "displayName" : "Pflegender", "level" : "1", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "Die Person, die den Patienten pflegerisch (aktiv) betreut.&lt;br clear=\"none\"/&gt;" }] }, { "code" : "17", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.13", "displayName" : "Begutachtender", "level" : "1", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "Die Person, die ein Gutachten erstellt." }] }, { "code" : "8", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.13", "displayName" : "Behandler", "level" : "1", "type" : "S", "desc" : [{ "language" : "de-DE", "#text" : "Die Person, die den Patienten aktiv behandelt im Sinne der Verbesserung (oder zumindest Beibehaltung) des gesundheitlichen Zustandes.&lt;br clear=\"none\"/&gt;" }] }, { "code" : "9", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.13", "displayName" : "Erstbehandler außerhalb einer Einrichtung", "level" : "2", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "Die Person, die den Patienten außerhalb der (eigenen) Einrichtung zuerst behandelt hat. Das kann - muss aber nicht - im Rahmen einer Notfallversorgung stattgefunden haben.&lt;br clear=\"none\"/&gt;" }] }, { "code" : "10", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.13", "displayName" : "Bereitstellender", "level" : "1", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "Die Person, die zusätzliche Mittel (Medikamente, Heil-/Hilfsmittel) bereitgestellt hat.&lt;br clear=\"none\"/&gt;" }] }, { "code" : "11", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.13", "displayName" : "Dokumentierender", "level" : "0", "type" : "S", "desc" : [{ "language" : "de-DE", "#text" : "Die Person, die übermittelte Informationen eingibt oder ein Gerät, das erzeugte Daten im System hinterlegt.&lt;br clear=\"none\"/&gt;" }] }, { "code" : "12", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.13", "displayName" : "dokumentierendes Gerät", "level" : "1", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "Das Gerät, welches erzeugte Daten im System hinterlegt.&lt;br clear=\"none\"/&gt;" }] }, { "code" : "13", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.13", "displayName" : "Validierer", "level" : "1", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "Die Person, die Korrektheit der hinterlegten Information überprüft und verifiziert hat.&lt;br clear=\"none\"/&gt;" }] }, { "code" : "14", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.13", "displayName" : "Gesetzlich Verantwortlicher", "level" : "1", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "Die Person, die vor dem Gesetz für die Korrektheit der Informationen verantwortlich ist und dafür haftet.&lt;br clear=\"none\"/&gt;" }] }, { "code" : "15", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.13", "displayName" : "Beratender", "level" : "0", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "Die Person, die Auswertungen vorgenommen und beratend involviert war.&lt;br clear=\"none\"/&gt;" }] }, { "code" : "16", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.13", "displayName" : "Informierender", "level" : "0", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "Eine Person, die zu den hier dokumentierten Informationen beigetragen hat.&lt;br clear=\"none\"/&gt;" }] }, { "code" : "101", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.14", "displayName" : "Hausarzt", "level" : "0", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "der Hausarzt (Primärversorger) des Patienten&lt;br clear=\"none\"/&gt;" }] }, { "code" : "102", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.14", "displayName" : "Patient", "level" : "0", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "der Patient selbst&lt;br clear=\"none\"/&gt;" }] }, { "code" : "103", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.14", "displayName" : "Arbeitgebervertreter", "level" : "0", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "ein Vertreter des Arbeitgebers des Patienten&lt;br clear=\"none\"/&gt;" }] }, { "code" : "104", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.14", "displayName" : "Primärbetreuer (langfristig)", "level" : "0", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "Die Person, die den Patienten normalerweise primär betreut. Beispiele: Langzeitpflege, Stammapotheke&lt;br clear=\"none\"/&gt;" }] }, { "code" : "105", "codeSystem" : "1.3.6.1.4.1.19376.3.276.1.5.14", "displayName" : "Kostenträgerverteter", "level" : "0", "type" : "L", "desc" : [{ "language" : "de-DE", "#text" : "ein Vertreter des Kostenträgers" }] }] }] }] }